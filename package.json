    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "babel-jest": "^30.0.0",
    "eslint": "^8.22.0",
    "eslint-config-godaddy": "^6.1.0",
    "eslint-plugin-jest": "^27.0.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-mocha": "^10.1.0",
    "jest": "^29.0.3",
    "jsdoc-to-markdown": "^7.1.1"
